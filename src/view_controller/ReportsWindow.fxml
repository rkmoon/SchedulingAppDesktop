<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="256.0" prefWidth="304.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view_controller.ReportsWindowController">
   <children>
      <VBox alignment="CENTER" prefHeight="290.0" prefWidth="304.0" spacing="10.0">
         <children>
            <Label text="Reports">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="50.0" />
               </VBox.margin>
            </Label>
            <Button mnemonicParsing="false" onAction="#openCustomerAppointments" text="Total Customer Appointments" />
            <Button mnemonicParsing="false" onAction="#openContactSchedule" text="Contact Schedule" />
            <Button mnemonicParsing="false" onAction="#openCustomersPerCountry" text="Customers per Country" />
         </children>
      </VBox>
   </children>
</AnchorPane>
